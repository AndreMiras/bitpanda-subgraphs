{
 "scripts": {
  "dependencies": "npm run dependencies:svsn",
  "dependencies:svsn": "yarn --cwd svsn install --frozen-lockfile",
  "build": "npm run build:schema && npm run build:graph",
  "build:schema": "tsx scripts/build-schema.ts",
  "build:graph": "npm run build:graph:svsn",
  "build:graph:svsn": "cd svsn && graph codegen && graph build",
  "test": "npm run test:svsn",
  "test:svsn": "cd svsn && graph test",
  "test:docker": "npm run test:svsn:docker",
  "test:svsn:docker": "npm run test:svsn -- --docker"
 },
 "devDependencies": {
  "@graphprotocol/graph-cli": "^0.97.1",
  "tsx": "^4.20.3"
 }
}
